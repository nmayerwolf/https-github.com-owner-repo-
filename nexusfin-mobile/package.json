{
  "name": "nexusfin-mobile",
  "version": "1.0.0",
  "private": true,
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:ios": "eas build --platform ios --profile production",
    "build:android": "eas build --platform android --profile production",
    "submit:ios": "eas submit --platform ios --profile production",
    "submit:android": "eas submit --platform android --profile production",
    "release:preflight": "bash ./scripts/release_preflight.sh",
    "release:store:dry": "DRY_RUN=1 RUN_SUBMIT=0 bash ./scripts/store_release.sh",
    "release:store": "DRY_RUN=0 RUN_SUBMIT=1 bash ./scripts/store_release.sh"
  },
  "dependencies": {
    "expo": "~52.0.0",
    "expo-constants": "~17.0.0",
    "expo-device": "~7.0.0",
    "expo-notifications": "~0.29.0",
    "expo-secure-store": "~14.0.0",
    "react": "18.3.1",
    "react-native": "0.76.3"
  },
  "devDependencies": {
    "babel-preset-expo": "~12.0.0"
  }
}
